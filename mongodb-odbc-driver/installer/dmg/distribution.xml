    <welcome file="WELCOME.rtf" mim-type="application/rtf" />
    <license file="LICENSE.rtf" mim-type="application/rtf" />
    <pkg-ref id="MongoDBODBC"/>
    <options customize="never" require-scripts="false"/>
    <choices-outline>
        <line choice="default">
